{
  "mcpServers": {
    "mcplanmanager": {
      "command": "python",
      "args": ["-m", "mcplanmanager.mcp_server"],
      "cwd": "/workspace/PlanManager",
      "description": "PlanManager AI Agent任务管理工具 - 魔搭平台版本",
      "env": {
        "PYTHONPATH": "/workspace/PlanManager",
        "PLAN_MANAGER_DATA_DIR": "/workspace/data"
      },
      "install": {
        "type": "git",
        "url": "https://github.com/donway19/MCPlanManager.git",
        "requirements": "requirements.txt"
      }
    }
  }
}